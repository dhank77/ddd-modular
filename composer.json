{
    "name": "vendor/ddd-modular-toolkit",
    "description": "A Laravel DDD Modular Toolkit for feature-based architecture.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Vendor\\DDDModularToolkit\\": "src/"
        },
        "files": [
            "src/Helpers/PathHelper.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\DDDModularToolkit\\Providers\\DDDModularToolkitServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^12.19",
        "illuminate/console": "^12.19"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
